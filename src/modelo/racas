	  $('#b-fechado').stop().hide();
	  $('#b-aberto').stop().slideDown();
	  $('#v-pipi').html('<iframe width="373" height="280" src="http://portaldodog.com.br/pipi.html" frameborder="0"></iframe>');
	});
  $('#btn-fechar').click(function() {
	  $('#b-aberto').stop().slideUp();
	  $('#b-fechado').stop().slideDown(600);
	  $('#v-pipi').html('');
	});	   
});
